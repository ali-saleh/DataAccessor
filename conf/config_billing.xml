<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "http://mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url" value="jdbc:mysql://localhost:3306/jbill1" />
				<property name="username" value="root" />
				<property name="password" value="123456" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
<!-- 		<mapper resource="db/billingdb/xml/custom/ItemMapperCustom.xml" /> -->
		<mapper resource="db/billingdb/xml/custom/ItemReportMapper.xml" />
		<mapper resource="db/billingdb/xml/custom/InvoiceReportMapper.xml" />
		<mapper resource="db/billingdb/xml/custom/PaymentReportMapper.xml" />
<!-- 		<package name="db.billingdb.xml.custom" /> -->
	</mappers>
</configuration>
